<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.zingzing.heartree.Setting_main"
    android:background="@drawable/bg">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="메인으로 돌아가기"
        android:id="@+id/button7"
        android:onClick="onClick_back"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="커플 정보 등록"
        android:id="@+id/button9"
        android:layout_alignBottom="@+id/button7"
        android:layout_toEndOf="@+id/button7"
        android:onClick="onClick_submit" />

    <ImageButton
        android:background="#00000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:src="@drawable/heart_small"
        android:nestedScrollingEnabled="false"
        android:layout_alignTop="@+id/textView6"
        android:layout_toEndOf="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="커플 애칭"
        android:id="@+id/textView4"
        android:layout_marginTop="100dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/editText_cn"
        android:layout_marginEnd="23dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="첫만남"
        android:id="@+id/textView5"
        android:layout_marginTop="33dp"
        android:layout_below="@+id/editText_cn"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="대표 사진"
        android:id="@+id/textView6"
        android:layout_marginTop="46dp"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView5" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText_cn"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignStart="@+id/button7" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/editText_fm"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignStart="@+id/editText_cn" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView_pic"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_alignEnd="@+id/editText_fm"
        android:layout_toEndOf="@+id/imageButton2" />

</RelativeLayout>
